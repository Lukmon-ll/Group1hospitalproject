@model IEnumerable<Group1hospitalproject.Models.ParkingSpotDto>
@{
    ViewBag.Title = "List";
}

<h2>Parking Spots</h2>
<div style="margin: 1em; text-align:right">
    <a href="/ParkingSpot/New" class="button">Add a Parking Spot</a>
</div>


@foreach (var parkingspot in Model)
{
    <h2>Spot Number: <a href="/ParkingSpot/Details/@parkingspot.ParkingSpotID">@parkingspot.SpotNumber</a></h2>
}

